# Note: the $VAR's allow this file to act as an envsubst template for ci/cd

export ARENASTATE_ETHERSCAN_API_KEY=$ARENASTATE_OP_GOERLI_ETHERSCAN_API_KEY
export ARENASTATE_ETHERSCAN_URL=$ARENASTATE_OP_GOERLI_ETHERSCAN_URL

export ARENASTATE_PROVIDER_API_KEY=$ARENASTATE_OP_GOERLI_PROVIDER_API_KEY
