export ARENASTATE_NFTSTORAGE_API_KEY=$ARENASTATE_NFTSTORAGE_API_KEY
export ARENASTATE_OPENAI_API_KEY=$ARENASTATE_OPENAI_API_KEY

export ARENASTATE_NFTSTORAGE_URL=https://api.nft.storage
