# This example is used by the cd. The variables are envsubsted in from
# pipeline secrets

export ARENASTATE_LOGLEVEL=DEBUG

export ARENASTATE_DEPLOY_GASPRICE=${ARENASTATE_OP_GOERLI_DEPLOY_GASPRICE:-0.001}
